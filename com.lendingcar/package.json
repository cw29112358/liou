    "recyclerlistview": "^1.3.4",
    "redux": "^4.0.0-rc.1",
    "redux-form": "^7.4.2",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "reselect-immutable-helpers": "^1.2.2",
    "validator": "^10.4.0",
    "vin-validator": "^1.0.0"
